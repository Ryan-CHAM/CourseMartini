<div class="container">

<div class="page-header" style="text-align: center;">
<br>
    <h1>Account Database</h1><br><br>
</div>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>User name</th>
      <th>Email</th>
      <th>Password</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.encrypted_password %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>

